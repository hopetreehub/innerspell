# Production Environment Variables
# This file contains production-specific environment variables
# These values should be set in your deployment environment (Firebase, Vercel, etc.)

# Force cache invalidation and disable middleware - 2025-07-28
NEXT_CACHE_DISABLED=1
MIDDLEWARE_CACHE_BUST=2025-07-28-force-clear
FORCE_BUILD_TIME=2025-07-28T19:14:00Z
NEXT_MIDDLEWARE_DISABLED=true

# Performance and monitoring
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Rate limiting (production values)
RATE_LIMIT_ENABLED=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging level
LOG_LEVEL=error

# Cache settings
CACHE_TTL=3600

# Security settings
SECURITY_HEADERS_ENABLED=true
CSP_ENABLED=true

# Production domain
NEXT_PUBLIC_SITE_URL=https://innerspell.com

# AI Configuration (production)
AI_MODEL_TEMPERATURE=0.7
AI_MAX_TOKENS=4096

# Firebase settings (production)
FIREBASE_SERVICE_ACCOUNT_PATH=/app/firebase-service-account.json

# Session configuration
SESSION_SECRET_KEY=
SESSION_MAX_AGE=2592000

# Email settings
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=

# Monitoring
SENTRY_DSN=
OTEL_EXPORTER_OTLP_ENDPOINT=